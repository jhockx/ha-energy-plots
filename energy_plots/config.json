{
  "name": "Energy Plots",
  "version": "0.12.1",
  "slug": "energy_plots",
  "description": "This addon makes plots for your energy data, specifically for electricity, solar and gas data. It is assumed you are running InfluxDB as long term data storage.",
  "arch": [
    "armv7"
  ],
  "url": "https://github.com/jhockx/ha-energy-plots",
  "startup": "application",
  "boot": "auto",
  "options": {
    "host": null,
    "port": null,
    "username": null,
    "password": null,
    "daily_gas_usage": "none",
    "daily_electricity_usage": "none",
    "daily_yield": "none",
    "daily_gas_usage_monthly_avg": "False",
    "daily_electricity_usage_monthly_avg": "False",
    "daily_yield_monthly_avg": "False",
    "predicted_solar": "none"
  },
  "schema": {
    "host": "str",
    "port": "int",
    "username": "str",
    "password": "str",
    "daily_gas_usage": "str",
    "daily_electricity_usage": "str",
    "daily_yield": "str",
    "daily_gas_usage_monthly_avg": "str",
    "daily_electricity_usage_monthly_avg": "str",
    "daily_yield_monthly_avg": "str",
    "predicted_solar": "str"
  },
  "ports": {
    "8000/tcp": 8000
  }
}